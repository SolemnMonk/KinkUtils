<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://www.oracle.com/webfolder/technetwork/jsc/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<display-name>kUtils</display-name>
	<filter>
		<filter-name>Wicket</filter-name>
		<filter-class>org.apache.wicket.protocol.http.WicketFilter</filter-class>
		<init-param>
			<param-name>applicationFactoryClassName</param-name>
			<param-value>org.wicketstuff.osgi.OsgiWebApplicationFactory</param-value>
		</init-param>
		<init-param>
			<param-name>wicket.osgi.application.name</param-name>
			<param-value>KUtilsApplication</param-value>
		</init-param>
		<init-param>
			<param-name>applicationClassName</param-name>
			<param-value>monk.solemn.kutils.core.KUtilsApplication</param-value>
		</init-param>
	</filter>

	<filter-mapping>
		<filter-name>Wicket</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
</web-app>